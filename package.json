  "bugs": {
    "url": "https://github.com/ccrase/mock-market-project/issues"
  },
  "homepage": "https://github.com/ccrase/mock-market-project#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^4.1.2",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.1",
    "nodemon": "^1.19.2"
  }
}
